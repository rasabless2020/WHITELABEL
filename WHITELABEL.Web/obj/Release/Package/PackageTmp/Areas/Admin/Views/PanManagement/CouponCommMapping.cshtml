@using NonFactors.Mvc.Grid;
@{
    ViewBag.Title = "CouponCommMapping";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal padding-top-20">
        <div class="row">
            <div style="background-color:#ccc; border-radius:5px; color:#3e50b4; padding:5px 20px 10px 20px; margin-bottom:20px;" class="col-md-12"><h3 style="color:#3e50b4"><i class="fas fa-angle-double-right"></i> Coupon Commission List</h3></div>

            <div class="col-md-12">
                <div class="panel">
                    <div class="panel-heading">
                        <div class="panel-title">
                            <div class="text-right col-md-12">
                                <div class="panel-title" align="right"><a href="@Url.Action("CouponCommissionMapping", "MemberCouponMaster", new { area = "Admin" })" class="btn btn-info btn-xs text-color-white"><span class="ion-plus"></span> Coupon Commission</a></div>
                            </div>
                        </div>
                    </div><!--.panel-heading-->
                    <div class="panel-body">
                        <div class="row example-row">
                            @Html.AjaxGrid(Url.Action("CouponCommIndexGrid", "MemberCouponMaster", new { area = "Admin" }))
                        </div><!--.row-->
                    </div><!--.panel-body-->

                </div><!--.panel-->
            </div><!--.col-md-12-->
        </div><!--.row-->
    </div>

}


