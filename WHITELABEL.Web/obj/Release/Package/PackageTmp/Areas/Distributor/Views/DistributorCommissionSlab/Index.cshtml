@using NonFactors.Mvc.Grid;
@using WHITELABEL.Web.Helper;
@model IEnumerable<WHITELABEL.Data.Models.TBL_WHITE_LEVEL_COMMISSION_SLAB>
@{
    ViewBag.Title = "Index";
    //Layout = "~/Areas/Distributor/Views/Shared/_DistributorLayout.cshtml";
    Layout = "~/Areas/Distributor/Views/Shared/_LayoutDistributor.cshtml";
}


@Scripts.Render("~/bundles/DistributorCommissionSlabDeactivateJs")
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal padding-top-20">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-heading">
                        <div class="card-title" align="right"><a href="@Url.Action("AddCommissionSlab", "DistributorCommissionSlab", new { area = "Distributor" })" class="btn btn-info btn-xs text-color-white"><span class="ion-plus"></span>Add New Commission</a></div>
                    </div><!--.panel-heading-->
                    <div class="card-body">
                        <div class="row example-row">
                            @Html.AjaxGrid(Url.Action("IndexGrid", "DistributorCommissionSlab", new { area = "Distributor" }))
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

}

