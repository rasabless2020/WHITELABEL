    <script>
        $("select").on('change', function () {
            var str = 0;
            var tot = 0;
            $(".passenger option:selected").each(function () {
                tot = $(this).text() * 1;
                str = str + tot;
            });
            $("#count").text(str);
        }).change();


        $(function () {
