    <div id="Super_progress" class="loading hidden">Loading&#8230;</div>
    <script type="text/javascript">
        $(document).ready(function () {
            $('#Super_progress').removeClass("hidden");
            $('#Super_progress').hide();
        })
        function OnSuccess(response) {
            var message = response;
            bootbox.alert({
                message: message,
                size: 'small',
                callback: function () {
                    console.log(message);
                }
            })
            //alert(message);
        }
        function OnFailure(response) {
            alert("Error occured.");
        }
    </script>

