
@{
    ViewBag.Title = "FlightSearchSingleList";
    Layout = "~/Areas/Merchant/Views/Shared/_RetailerLayout.cshtml";
}

<div class="content" style="margin-top:20px;">
    <div class="display-animation">
        <div class="full-content margin-bottom-40 bg-white" id="Bill_page">
            <div class="row example-row" style="min-height:350px;">
                <div class="card" style="width:100%;">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-md-3 material-animate padding-right-40">
                                <ul class="list-group ">
                                    <li class="list-group-item">
                                        <div class="form-group">
                                            <label>Price range:</label>
                                            <input type="text" name="" class="ionrange-money1">
                                        </div>
                                        <!--.row-->
                                    </li>
                                    <li class="list-group-item">
                                        Time slots for the flight
                                        <div class="checkboxer">
                                            <input type="checkbox" value="" id="check1">
                                            <label for="check1"> Early Morning <small>12am - 8am</small></label>
                                        </div>
                                        <!--.checkbox-->

                                        <div class="checkboxer">
                                            <input type="checkbox" value="" id="check2">
                                            <label for="check2"> Morning <small>8am - 12pm</small></label>
                                        </div>
                                        <!--.checkbox-->

                                        <div class="checkboxer">
                                            <input type="checkbox" value="" id="check3">
                                            <label for="check3">Mid-Day <small>12pm - 4pm</small></label>
                                        </div>
                                        <!--.checkbox-->

                                        <div class="checkboxer">
                                            <input type="checkbox" value="" id="check4">
                                            <label for="check4">Evening <small>4pm - 8pm</small></label>
                                        </div>
                                        <!--.checkbox-->

                                        <div class="checkboxer">
                                            <input type="checkbox" value="" id="check5">
                                            <label for="check5">Night <small>8pm - 12am</small></label>
                                        </div>
                                        <!--.checkbox-->

                                    </li>
                                    <li class="list-group-item">
                                        Stops for that flight
                                        <div class="clearfix"></div>
                                        <div class="btn-group btn-group-xs" data-toggle="buttons">
                                            <label class="btn btn-blue-grey active">
                                                <input type="radio" name="options" id="option1" autocomplete="off" checked>
                                                1
                                            </label>
                                            <label class="btn btn-blue-grey">
                                                <input type="radio" name="options" id="option2" autocomplete="off">
                                                2
                                            </label>
                                            <label class="btn btn-blue-grey">
                                                <input type="radio" name="options" id="option3" autocomplete="off">
                                                3
                                            </label>
                                        </div>
                                    </li>
                                    <li class="list-group-item">
                                        Refundable:
                                        <div class="btn-group btn-group-xs" data-toggle="buttons">
                                            <label class="btn btn-blue-grey active">
                                                <input type="radio" name="optionsa" id="option1" autocomplete="off" checked>
                                                yes
                                            </label>
                                            <label class="btn btn-blue-grey">
                                                <input type="radio" name="optionsa" id="option2" autocomplete="off">
                                                No
                                            </label>
                                        </div>
                                    </li>
                                    <li class="list-group-item">
                                        Checkbox:
                                        <div class="checkboxer">
                                            <input type="checkbox" value="" id="check6">
                                            <label for="check6"> For all Airlines</label>
                                        </div>
                                        <!--.checkbox-->

                                    </li>
                                    <li class="list-group-item">
                                        <div class="form-group">
                                            <label>Promo Code </label>
                                            <input class="form-control" type="text" placeholder="promo">
                                        </div>
                                    </li>
                                </ul>
                            </div>
